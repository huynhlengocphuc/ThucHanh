      "source": [
        "from sympy import Symbol,solve, Derivative\n",
        "x=Symbol('x')\n",
        "f=-x**2+4*x-3\n",
        "d1=Derivative(f,x).doit()\n",
        "cuctri=solve(d1)\n",
        "cuctri\n",
        "[2]\n",
        "A=cuctri[0]\n",
        "d2=Derivative(d1,x).doit()\n",
        "d2.subs({x:A}).evalf()\n",
        "\n"
      ],
